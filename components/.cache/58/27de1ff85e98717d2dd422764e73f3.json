onents/KanbanCard.vue","name":null,"original":{"line":87,"column":0},"generated":{"line":94,"column":0}},{"source":"src/components/KanbanCard.vue","name":null,"original":{"line":84,"column":0},"generated":{"line":95,"column":0}},{"source":"src/components/KanbanCard.vue","name":null,"original":{"line":45,"column":0},"generated":{"line":96,"column":0}},{"source":"src/components/KanbanCard.vue","name":null,"original":{"line":42,"column":0},"generated":{"line":97,"column":0}}],"sources":{"src/components/KanbanCard.vue":"<template>\n  <div class=\"card border-black\">\n    <p>Title :{{ card.title }}</p>\n    <p>Category :{{ card.category }}</p>\n    <p>Description : {{ card.description }}</p>\n    <p>Due Date: {{ card.due_date }}</p>\n    \n    <div class=\"flex-button\" >\n      <button\n        class=\"flex btn-info\"\n        href=\"#\"\n        data-toggle=\"modal\"\n        data-target=\"#updateTaskModal\"\n      >\n        Update\n      </button>\n      <button\n        class=\"flex btn-warning\"\n        v-if=\"card.status > 1\"\n        @click=\"changeStatus(card.id, card.status - 1)\"\n      >\n        Back\n      </button>\n      <button\n        class=\"flex btn-success\"\n        v-if=\"card.status <= 3\"\n        @click=\"changeStatus(card.id, card.status + 1)\"\n      >\n        Next\n      </button>\n      <button class=\"flex btn-danger\" @click=\"deleteCard(card.id)\">\n        Delete\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nconst baseUrl = \"https://shrouded-falls-16636.herokuapp.com\";\n\nexport default {\n  name: \"KanbanCard\",\n  props: [\"card\"],\n  methods: {\n    deleteCard(id) {\n      let reConfirm = confirm(`You sure want to destroy it ?`);\n\n      if (reConfirm) {\n        axios({\n          method: \"DELETE\",\n          url: `${baseUrl}/tasks/${id}`,\n          headers: {\n            access_token: localStorage.getItem(\"access_token\"),\n          },\n        })\n          .then((_) => {\n            this.$emit(\"fetchData\");\n          })\n          .catch((err) => {\n            console.log(err);\n          });\n      }\n    },\n\n    changeStatus(id, statusId) {\n      axios({\n        method: \"PATCH\",\n        url: `${baseUrl}/tasks/${id}`,\n        data: {\n          status: statusId,\n        },\n        headers: {\n          access_token: localStorage.getItem(\"access_token\"),\n        },\n      })\n        .then((_) => {\n          this.$emit(\"fetchData\");\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    computed: {\n      convertDate() {\n        return moment(this.card.due_date).format(\"YYYY-MM-DD\");\n      },\n    },\n  },\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"a900b4fd8b85ff43f922e35335146b96","cacheData":{"env":{}}}