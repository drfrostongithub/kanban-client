  method: \"DELETE\",\n          url: `${baseUrl}/tasks/${id}`,\n          headers: {\n            access_token: localStorage.getItem(\"access_token\"),\n          },\n        })\n          .then((_) => {\n            this.$emit(\"fetchData\");\n          })\n          .catch((err) => {\n            this.error = err.msg\n            console.log(err);\n          });\n      }\n    },\n\n    changeStatus(id, statusId) {\n      axios({\n        method: \"PATCH\",\n        url: `${baseUrl}/tasks/${id}`,\n        data: {\n          status: statusId,\n        },\n        headers: {\n          access_token: localStorage.getItem(\"access_token\"),\n        },\n      })\n        .then((_) => {\n          this.$emit(\"fetchData\");\n        })\n        .catch((err) => {\n          this.error = err.msg\n          console.log(err);\n        });\n    }\n  }\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"9d794aa258a2be1a7e140d98c4c5b695","cacheData":{"env":{}}}